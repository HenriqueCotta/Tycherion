# Local development environment variables.
# Never commit real credentials. Keep secrets only in local .env or CI secret store.

# Optional runner identity for telemetry correlation
TYCHERION_RUNNER_ID=dev-runner-01

# MetaTrader 5 credentials
MT5_TERMINAL_PATH=
MT5_SERVER=
MT5_LOGIN=
MT5_PASSWORD=

# Optional observability overrides
TYCHERION_CONSOLE_ENABLED=true
TYCHERION_CONSOLE_MIN_LEVEL=INFO
TYCHERION_CONSOLE_CHANNELS=ops
TYCHERION_OTLP_ENABLED=false
TYCHERION_OTLP_ENDPOINT=http://localhost:4317
TYCHERION_OTLP_PROTOCOL=grpc
